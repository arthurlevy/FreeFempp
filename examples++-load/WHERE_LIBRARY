f77 LD /usr/lib/gcc/x86_64-linux-gnu/4.8/libgfortran.so
f77 INCLUDE -DAdd_  
fc LD /usr/lib/gcc/x86_64-linux-gnu/4.8/libgfortran.so
fc INCLUDE -DAdd_  
pthread LD -lpthread
mpifc LD -pthread -L/usr/lib/openmpi/lib -lmpi_f90 -lmpi_f77 -lmpi -lopen-rte -lopen-pal -ldl -Wl,--export-dynamic -lnsl -lutil -lm -ldl 
mpifc INCLUDE -I/usr/lib/openmpi/include -I/usr/lib/openmpi/include/openmpi  
mpif77 LD -pthread -L/usr/lib/openmpi/lib -lmpi_f90 -lmpi_f77 -lmpi -lopen-rte -lopen-pal -ldl -Wl,--export-dynamic -lnsl -lutil -lm -ldl 
mpif77 INCLUDE -I/usr/lib/openmpi/include -I/usr/lib/openmpi/include/openmpi  
mpi LD -pthread -L/usr/lib/openmpi/lib -lmpi_cxx -lmpi -lopen-rte -lopen-pal -ldl -Wl,--export-dynamic -lnsl -lutil -lm -ldl 
mpi INCLUDE -I/usr/lib/openmpi/include -I/usr/lib/openmpi/include/openmpi  
fftw3 LD -lfftw3
blas LD -lblas
lapack LD -llapack
arpack LD -larpack -llapack
arpack LD '-larpack -llapack'
amd LD  -lumfpack  -lamd
amd INCLUDE -I/usr/include/suitesparse 
umfpack LD  -lumfpack  -lamd
umfpack INCLUDE -I/usr/include/suitesparse 
mumps_seq LD -ldmumps_seq -lzmumps_seq  -lmumps_common_seq  -lpord_seq
mumps_seq INCLUDE -I/usr/include
mumps LD -ldmumps -lzmumps  -lmumps_common  -lpord
mumps INCLUDE -I/usr/include
mumps_ptscotch LD -lpord_ptscotch -lmumps_common_ptscotch -ldmumps_ptscotch -lzmumps_ptscotch -lpord_ptscotch
mumps_ptscotch INCLUDE -I/usr/include
mumps_scotch LD -lpord_scotch -lmumps_common_scotch -ldmumps_scotch -lzmumps_scotch -lpord_scotch
mumps_scotch INCLUDE -I/usr/include
hypre LD -lHYPRE
superlu LD -lsuperlu
superlu INCLUDE -I/usr/include/superlu
blacs LD  -lblacsCinit-openmpi	-lblacsF77init-openmpi	-lblacs-openmpi
scalapack LD -lscalapack-openmpi
ptscotch LD -lptscotch -lptscotcherr
metis LD -lscotch -lmetis
metis INCLUDE -I/usr/include/metis
parmetis LD -lptscotch -lparmetis
gsl LD -lgsl -lgslcblas -lm
gsl INCLUDE -I/usr/include/gsl
nlopt LD -lnlopt
nlopt INCLUDE -I/usr/include
