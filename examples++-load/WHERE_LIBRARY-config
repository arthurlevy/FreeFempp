f77 LD /usr/lib/gcc/x86_64-linux-gnu/4.4.5/libgfortran.so
f77 INCLUDE -DAdd_
fc LD /usr/lib/gcc/x86_64-linux-gnu/4.4.5/libgfortran.so
fc INCLUDE -DAdd_
mpifc LD -pthread -L/usr/lib/openmpi/lib -lmpi_f90 -lmpi_f77 -lmpi -lopen-rte -lopen-pal -ldl -Wl,--export-dynamic -lnsl -lutil -lm -ldl
mpifc INCLUDE -I/usr/lib/openmpi/include -I/usr/lib/openmpi/lib
mpif77 LD -pthread -L/usr/lib/openmpi/lib -lmpi_f90 -lmpi_f77 -lmpi -lopen-rte -lopen-pal -ldl -Wl,--export-dynamic -lnsl -lutil -lm -ldl
mpif77 INCLUDE -I/usr/lib/openmpi/include -I/usr/lib/openmpi/lib
mpi LD -pthread -L/usr/lib/openmpi/lib -lmpi_cxx -lmpi -lopen-rte -lopen-pal -ldl -Wl,--export-dynamic -lnsl -lutil -lm -ldl
mpi INCLUDE -I/usr/lib/openmpi/include -I/usr/lib/openmpi/include/openmpi
fftw3 LD -lfftw3
blas LD '-lblas'
lapack LD '-llapack'
arpack LD '-larpack -llapack'
amd LD ' -lumfpack -lamd'
amd INCLUDE '-I/usr/include/suitesparse'
umfpack LD $ff_umfpack_libs\
umfpack INCLUDE '-I/usr/include/suitesparse'
mumps LD '-ldmumps -lzmumps  -lmumps_common  -lpord'
